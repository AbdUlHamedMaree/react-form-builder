{
  "name": "@mrii/react-form-builder",
  "version": "0.2.0",
  "private": false,
  "sideEffects": false,
  "description": "library to easily build forms using react-hook-form, MUI & yup",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdUlHamedMaree/react-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/AbdUlHamedMaree/react-form-builder/issues"
  },
  "homepage": "https://github.com/AbdUlHamedMaree/react-form-builder.git",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "email": "hmidmrii@gmail.com",
    "name": "Abd UlHameed Maree",
    "url": "https://www.linkedin.com/in/abd-ul-hameed-maree"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design",
    "yup",
    "validation",
    "form",
    "forms",
    "form-input",
    "react-hook-form"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c && yarn tsc",
    "build:watch": "concurrently \"rollup -c -w\" \"yarn tsc -w\"",
    "example:next": "concurrently \"yarn build:watch\" \"cd ./nextjs-example && yarn dev\"",
    "example:react": "concurrently \"yarn build:watch\" \"cd ./reactjs-example && yarn start\"",
    "test": "jest",
    "lint": "yarn type-check && yarn eslint --quiet",
    "lint:fix": "yarn eslint --fix",
    "type-check": "tsc --noEmit -p ./tsconfig.check.json",
    "tsc": "tsc",
    "eslint": "eslint . --config ./.eslintrc.json",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.6.2",
    "@mui/system": "^5.6.2",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@swc/core": "^1.2.171",
    "@swc/jest": "^0.2.20",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "next": "^12.1.5",
    "prettier": "^2.6.2",
    "quill": "^1.3.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.30.0",
    "react-quill": "^1.3.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.3.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.6.2",
    "@mui/system": "^5.6.2",
    "next": "^12.1.5",
    "quill": "^1.3.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.30.0",
    "react-quill": "^1.3.5",
    "yup": "^0.32.11"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "quill": {
      "optional": true
    },
    "react-dropzone": {
      "optional": true
    },
    "react-quill": {
      "optional": true
    }
  }
}
