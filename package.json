{
  "name": "@mrii/react-form-builder",
  "version": "0.2.2",
  "private": false,
  "sideEffects": false,
  "description": "library to easily build forms using react-hook-form, MUI & yup",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdUlHamedMaree/react-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/AbdUlHamedMaree/react-form-builder/issues"
  },
  "homepage": "https://github.com/AbdUlHamedMaree/react-form-builder.git",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "email": "hmidmrii@gmail.com",
    "name": "Abd UlHameed Maree",
    "url": "https://www.linkedin.com/in/abd-ul-hameed-maree"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design",
    "yup",
    "validation",
    "form",
    "forms",
    "form-input",
    "react-hook-form"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently --kill-others-on-fail -n rollup,tsc -c magenta,cyan \"rollup -c\" \"yarn tsc\"",
    "build:watch": "concurrently -k -n rollup,tsc -c magenta,cyan \"rollup -c -w\" \"yarn tsc -w\"",
    "example:next:dev": "concurrently -k -n build,dev -c magenta,cyan \"yarn build:watch -p none\" \"yarn --cwd nextjs-example dev\"",
    "example:react:dev": "concurrently -k -n build,dev -c magenta,cyan \"yarn build:watch -p none\" \"yarn --cwd reactjs-example start\"",
    "test": "jest",
    "lint": "concurrently --kill-others-on-fail -n tsc,eslint -c magenta,cyan \"yarn type-check\" \"yarn eslint --quiet\"",
    "lint:fix": "concurrently --kill-others-on-fail -n tsc,eslint -c magenta,cyan \"yarn type-check\" \"yarn eslint --fix --quiet\"",
    "type-check": "tsc -p ./tsconfig.eslint.json",
    "tsc": "tsc",
    "eslint": "eslint .",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepack": "yarn lint:fix && yarn build",
    "prepare": "ts-patch install -s"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@mui/x-date-pickers": "^5.0.4",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@swc/core": "^1.3.6",
    "@swc/jest": "^0.2.23",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.1.2",
    "next": "^12.3.1",
    "prettier": "^2.7.1",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.37.0",
    "react-quill": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.6.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "ts-patch": "^2.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@mui/x-date-pickers": "^5.0.4",
    "next": "^12.3.1",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.37.0",
    "react-quill": "^2.0.0",
    "yup": "^0.32.11"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "quill": {
      "optional": true
    },
    "react-dropzone": {
      "optional": true
    },
    "react-quill": {
      "optional": true
    }
  }
}
